{
  "name": "meteor/epfl:accounts-oidc",
  "version": "0.1.0",
  "devDependencies": {
    "@types/meteor": "^2.9.10",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0"
  },
  "exports": "index.ts",
  "scripts": {
    "api": "typedoc"
  },
  "typedocOptions": {
    "plugin": ["typedoc-plugin-markdown"],
    "router": "module",
    "excludeInternal": true,
    "blockTags": ["@param", "@return", "@locus",
                  "@typeParam", "@inlineType", "@property", "@prop",
                  "@defaultValue", "@example", "@see"],
    "sort": "source-order",
    "readme": "none",
    "out": "api",
    "entryFileName": "API.md"
  }
}
